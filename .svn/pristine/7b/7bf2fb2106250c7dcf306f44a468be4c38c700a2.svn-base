<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tingpan.grpproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tingpan.grpproject.Launch"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tingpan.grpproject.ChooseGame"
            android:label="@string/title_activity_choose_game"
            android:parentActivityName="com.example.tingpan.grpproject.Launch" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launch" >
            </meta-data>
        </activity>
        <activity
            android:name="com.example.tingpan.grpproject.ReadyToPort"
            android:label="@string/title_activity_ready_to_port"
            android:parentActivityName="com.example.tingpan.grpproject.Launch" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launch" >
            </meta-data>
        </activity>
        <activity
            android:name="com.example.tingpan.grpproject.PortGame"
            android:label="@string/title_activity_portgame" >
        </activity>
    </application>

</manifest>